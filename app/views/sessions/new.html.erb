<section class="sessions-new">
  <header class="page-header">
    <h1>Login</h1>
  </header>

  <%= form_for '/login' do |f| %>
    <%= f.label :email %>
    <%= f.text_field :email%>
    <%= f.label :password %>
    <%= f.password_field :password %>    
    <%= f.submit "Login" %>
  <% end %>
</section>